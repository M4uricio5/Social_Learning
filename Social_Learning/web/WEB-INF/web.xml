<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>paquete.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>menu</servlet-name>
        <servlet-class>paquete.menu</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>barraR</servlet-name>
        <servlet-class>paquete.barraR</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>subMenuR</servlet-name>
        <servlet-class>paquete.subMenuR</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editPR</servlet-name>
        <servlet-class>paquete.editPR</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>menu</servlet-name>
        <url-pattern>/menu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>barraR</servlet-name>
        <url-pattern>/barraR</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>subMenuR</servlet-name>
        <url-pattern>/subMenuR</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editPR</servlet-name>
        <url-pattern>/editPR</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
